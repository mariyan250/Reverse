{
  "setupFiles": ["<rootDir>/src/tests/setup-tests.js"],
  "testRegex": "/*.spec.js$",
  "collectCoverage": true,
  "coverageReporters": ["lcov"],
  "coverageDirectory": "test-coverage",
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "moduleDirectories": ["node_modules", "src"]
}
